package com.nhaowan.mobile.activity.bean;

import com.nhaowan.mobile.base.BaseContentFragment;
import com.nhaowan.mobile.base.BaseFragment;

public class TopCategory {
	private int id;
	private String name;
	private int thumbId;
	private String description;
	private Class<? extends BaseContentFragment> clazz;
	private boolean hasUpdate;
	private boolean isChecked;

	public TopCategory() {
		// TODO Auto-generated constructor stub
	}

	public TopCategory(int id, String name, int thumb, String description,
			Class<? extends BaseContentFragment> clazz, boolean hasUpdate) {
		super();
		this.id = id;
		this.name = name;
		this.thumbId = thumb;
		this.description = description;
		this.clazz = clazz;
		this.hasUpdate = hasUpdate;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getThumbId() {
		return thumbId;
	}

	public void setThumbId(int thumb) {
		this.thumbId = thumb;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public Class<? extends BaseContentFragment> getClazz() {
		return clazz;
	}

	public void setClazz(Class<? extends BaseContentFragment> clazz) {
		this.clazz = clazz;
	}

	public boolean isHasUpdate() {
		return hasUpdate;
	}

	public void setHasUpdate(boolean hasUpdate) {
		this.hasUpdate = hasUpdate;
	}

	public boolean isChecked() {
		return isChecked;
	}

	public void setChecked(boolean isChecked) {
		this.isChecked = isChecked;
	}

}
